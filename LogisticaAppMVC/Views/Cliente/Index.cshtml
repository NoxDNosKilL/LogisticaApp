@{
    ViewData["Title"] = "Index";
}
<div class="text-center">
    <h1 class="display-4">Agregar Cliente</h1>
</div>

<div class="container-fluid">
    <div class="row align-content-center">

        <div class="col-md-6">
            <div class="form-group">
                <label>Nombre</label>
                <input type="text" id="txtNombre" class="form-control" width="200" />
            </div>

            <div class="form-group">
                <label>Apellido</label>
                <input type="text" id="txtApellido" class="form-control" width="200" />
            </div>

            <div class="form-group">
                <label>Email</label>
                <input type="email" id="txtEmail" class="form-control" width="200" />
            </div>

            <div class="form-group">
                <label>Teléfono</label>
                <input type="text" id="txtTelefono" class="form-control" width="200" />
            </div>

            <div class="form-group">
                <label>Dirección</label>
                <input type="text" id="txtDireccion" class="form-control" width="200" />
            </div>

            <button id="btnAgregarCliente" class="btn btn-success">Agregar Cliente</button>
        </div>

    </div>
</div>

<script src="~/lib/ns.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/js/Cliente/Index.js"></script>